<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/profile"
    app:startDestination="@id/profile_fragment">

    <fragment
        android:id="@+id/profile_fragment"
        android:name="com.lineupkita.app.ui.profile.ProfileFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragmanet_profile" >


        <action
            android:id="@+id/action_profile_fragment_to_whislist_fragment"
            app:destination="@id/whislist_fragment" />
        <action
            android:id="@+id/action_profile_fragment_to_artist_fav_fragment"
            app:destination="@id/artist_fav_fragment" />

    </fragment>

    <fragment
        android:id="@+id/whislist_fragment"
        android:name="com.lineupkita.app.ui.whislist.WhislistFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_whislist" >

    </fragment>

    <fragment
        android:id="@+id/artist_fav_fragment"
        android:name="com.lineupkita.app.ui.artisfavorite.ArtisFavoriteFragment"
        android:label="@string/nav_home"
        tools:layout="@layout/fragment_artist_favorite" >

    </fragment>


</navigation>